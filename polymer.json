{
  "entrypoint": "index.html",
  "shell": "src/vaadin-elements-app.html",
  "sources": [
   "src/**/*"
  ],
  "extraDependencies": [
    "images/**/*",
    "manifest.json",
    "employees.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/webcomponentsjs/*.js.map",
    "!bower_components/webcomponentsjs/gulpfile.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "autoBasePath": true,
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
